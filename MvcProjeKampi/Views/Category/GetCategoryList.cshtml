@using EntityLayer.Concrete
@model List<Category>
@{
    Layout = null;
}

<!DOCTYPE html>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<br />
<table class="table table-bordered">
    <tr>
       <th>ID</th>
       <th>Ad</th>
       <th>Durum</th>
    </tr>
    @foreach(var x in Model)
            {
        <tr>
            <td>@x.CategoryID</td>
            <td>@x.CategoryName</td>            
            <td>@x.CategoryStatus</td>
        </tr>
            }    
</table>
<a href="/Category/CategoryAdd" class="btn btn-danger">Yeni Kategori</a>